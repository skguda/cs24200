knn <- function(data, xseq, k) {    
    ###
    ### YOUR CODE HERE
    ###  
    count <- list()
    for (num in xseq) {
        data$diff <- abs(num - data[,1]) 
        newdata <- data[order(data$diff),] 
        newdata <- newdata[1:k,] 
        count[[toString(num)]] <- sum(newdata$y)/k   
    }   
    return(unlist(count))
    
    
 
}  
