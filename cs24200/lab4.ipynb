​
readIn_standardize <- function(filename) {
​
    ###
    ### YOUR CODE HERE
    ### 
    myDF <- read.csv(filename) 
    newDF <- subset(myDF, select = c(6))
    colMean <- mean(newDF[,1])
    colSD <- sd(newDF[,1])  
    vec <- c((newDF - colMean) / colSD) 
    return(unlist(vec))
    
​
​
    
}
